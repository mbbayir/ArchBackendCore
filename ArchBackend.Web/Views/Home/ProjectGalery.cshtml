
@{
    ViewData["Title"] = "ProjectGalery";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content -->
<div class="content-wrapper">
    <!-- Lines -->
    <section class="content-lines-wrapper">
        <div class="content-lines-inner">
            <div class="content-lines"></div>
        </div>
    </section>
    <!-- Header Banner -->
    <section class="banner-header banner-img valign bg-img bg-fixed" data-overlay-light="3" data-background="@Url.Content("~/img/banner.jpg")">
        <div class="left-panel"></div>
    </section>
    <!-- Project Gallery -->
    <section class="section-padding2">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <h2 class="section-title">Project <span>Gallery</span></h2>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">
                    <ul class="bauen-gallery-filter">
                        <li class="active" data-filter="*">All</li>
                        <li data-filter=".projects-progress">Projects in Progress</li>
                        <li data-filter=".completed-projects">Completed Projects</li>
                    </ul>
                </div>
            </div>
            <div class="row bauen-gallery-items">
                <div class="col-md-6 gallery-masonry-wrapper single-item projects-progress">
                    <a href="~/img/projects/1.jpg" title="Cotton House" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/1.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Cotton House</h4>
                                <div class="gallery-masonry-item-category">Interior</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 gallery-masonry-wrapper single-item completed-projects">
                    <a href="~/img/projects/3.jpg" title="Stonya Villa" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/3.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Stonya Villa</h4>
                                <div class="gallery-masonry-item-category">Interior</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 gallery-masonry-wrapper single-item projects-progress">
                    <a href="~/img/projects/4.jpg" title="Prime Hotel" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/4.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Prime Hotel</h4>
                                <div class="gallery-masonry-item-category">Urban</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 gallery-masonry-wrapper single-item completed-projects">
                    <a href="~/img/projects/5.jpg" title="Geometric Addo" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/5.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Geometric Addo</h4>
                                <div class="gallery-masonry-item-category">Urban</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 gallery-masonry-wrapper single-item projects-progress">
                    <a href="~/img/projects/2.jpg" title="Armada Center" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/2.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Armada Center</h4>
                                <div class="gallery-masonry-item-category">Exterior</div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="col-md-6 gallery-masonry-wrapper single-item completed-projects">
                    <a href="~/img/projects/6.jpg" title="Twins Office" class="gallery-masonry-item-img-link img-zoom">
                        <div class="gallery-box">
                            <div class="gallery-img"> <img src="~/img/projects/6.jpg" class="img-fluid mx-auto d-block" alt="work-img"> </div>
                            <div class="gallery-masonry-item-img"></div>
                            <div class="gallery-masonry-item-content">
                                <h4 class="gallery-masonry-item-title">Twins Office</h4>
                                <div class="gallery-masonry-item-category">Interior</div>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </section>
